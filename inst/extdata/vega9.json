{
  "description": "Stock prices of 5 Tech Companies Over Time.",
  "data": {
    "url": "https://vega.github.io/vega-lite/data/stocks.csv",
    "formatType": "csv"
  },
  "mark": "line",
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal"
    },
    "y": {
      "field": "price",
      "type": "quantitative"
    },
    "color": {
      "field": "symbol",
      "type": "nominal"
    }
  }
}